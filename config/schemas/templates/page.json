{
  "name": "Pages",
  "previewUrl": "",
  "request": [
    {
      "url": "/api/pagebuilder/search?storeId=B2B-store&type=pages&query=*.page",
      "method": "get",
      "cacheable": true,
      "response": {
        "selector": "Object.keys(this.response).map(x => ({ ...this.item, ...this.response[x], request: undefined })).reduce((acc, x) => ({ ...acc, [x.name]: x }), {})",
        "isArray": true
      }
    }
  ],
  "type": "pages",
  "sections": [
    "image",
    "text",
    "seo-settings",
    "side-image",
    "promo-text",
    "column-images",
    "dark-promo-text",
    "subscribe-form",
    "products"
  ],
  "settings": [
    {
      "id": "header",
      "label": "Page header",
      "type": "string"
    }
  ]
}
