{
  "name": "vc-theme-b2b-vue",
  "version": "1.32.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "yarn type-check && vite build",
    "build:dev": "vite build --mode=development",
    "build:watch": "vite build --mode=development --watch",
    "compress": "yarn build && gulp compress",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint-staged": "lint-staged",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "generate:graphql:schema": "graphql-codegen -c \"codegen.schema.ts\" -r dotenv/config",
    "generate:graphql:types": "graphql-codegen -c \"codegen.types.ts\" -r dotenv/config",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "storybook:dev": "start-storybook -p 6006 --no-open",
    "storybook:build": "build-storybook --docs",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@headlessui/vue": "^1.7.13",
    "@popperjs/core": "^2.11.8",
    "@vee-validate/rules": "^4.9.5",
    "@vee-validate/yup": "^4.9.5",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "~0.9.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.30.2",
    "dompurify": "^3.0.3",
    "flag-icons": "^6.7.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "~4.3.0",
    "maska": "~1.5.2",
    "moment": "^2.29.4",
    "swiper": "^8.4.7",
    "vee-validate": "^4.9.5",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.2",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@rollup/plugin-graphql": "^2.0.3",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/cli": "^6.5.16",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^6.5.16",
    "@types/dompurify": "^3.0.2",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash": "^4.14.195",
    "@types/marked": "^4.3.1",
    "@types/node": "^18.16.16",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "babel-loader": "~8.3.0",
    "eslint": "^8.41.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "eslint-plugin-vue": "^9.14.1",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "optionator": "^0.9.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "storybook-vue3-router": "^2.3.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.9",
    "vite-plugin-checker": "0.6.0",
    "vite-plugin-mkcert": "^1.15.0",
    "vue-eslint-parser": "^9.3.0",
    "vue-loader": "^17.1.2",
    "vue-tsc": "1.6.5",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "resolutions": {
    "@types/react": "file:stub/@types/react"
  }
}
