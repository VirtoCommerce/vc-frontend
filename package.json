{
  "name": "vc-theme-b2b-vue",
  "version": "1.13.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "yarn run type-check && vite build",
    "builder": "vite build --mode=development",
    "compress": "yarn run build && gulp compress",
    "type-check": "vue-tsc --noEmit --extendedDiagnostics -p tsconfig.json --composite false",
    "lint-staged": "lint-staged",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "generate:graphql:schema": "graphql-codegen -c \"codegen.schema.ts\" -r dotenv/config && eolConverter crlf \"client-app/xapi/schema.json\"",
    "generate:graphql:types": "graphql-codegen -c \"codegen.types.ts\" -r dotenv/config && eolConverter crlf \"client-app/xapi/types.ts\"",
    "generate:graphql": "yarn run generate:graphql:schema && yarn run generate:graphql:types"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@headlessui/vue": "^1.6.4",
    "@popperjs/core": "^2.11.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@vueuse/core": "^9.2.0",
    "@vueuse/head": "^0.7.10",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.25.2",
    "dompurify": "^2.3.8",
    "flag-icons": "^6.4.4",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^4.0.16",
    "maska": "^1.5.0",
    "moment": "^2.29.3",
    "swiper": "^8.3.2",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.41",
    "vue-i18n": "^9.1.10",
    "vue-router": "~4.0.16",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.13.8",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@rollup/plugin-graphql": "^1.1.0",
    "@types/dompurify": "^2.3.3",
    "@types/lodash": "^4.14.185",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "cross-env": "^7.0.3",
    "eol-converter-cli": "^1.0.8",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "fse": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "sass": "^1.52.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.8.3",
    "vite": "^3.1.8",
    "vite-plugin-checker": "^0.5.1",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^1.0.8",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
