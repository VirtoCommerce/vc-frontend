{
  "name": "vc-theme-b2b-vue",
  "author": {
    "email": "info@virtocommerce.com",
    "name": "Virto Commerce",
    "url": "https://virtocommerce.com"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "version": "1.57.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:expose": "yarn dev --host",
    "build": "yarn validate && yarn generate:routes && yarn build-only",
    "build:dev": "yarn build-only --mode=development",
    "build:watch": "yarn build-only --watch",
    "build-only": "vite build",
    "compress": "vite-node --mode production scripts/compress-theme.ts --",
    "validate:types": "vue-tsc --build --force",
    "validate:dependencies": "depcruise client-app",
    "validate": "yarn validate:types && yarn validate:dependencies",
    "commitlint": "commitlint --edit",
    "lint-staged": "lint-staged",
    "lint": "eslint . --ext .js,.csj,.ts,.cts,.vue --ignore-path .eslintignore",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --check client-app/",
    "prettier:fix": "yarn prettier --write",
    "test:unit": "vitest",
    "test:typing": "vitest --typecheck",
    "generate:certificates": "vite-node --mode production scripts/generate-certificates",
    "generate:graphql:schema": "graphql-codegen --config graphql-codegen.schema.config.ts --require ./scripts/graphql-codegen.cjs",
    "generate:graphql:types": "graphql-codegen --config graphql-codegen.types.config.ts --require ./scripts/graphql-codegen.cjs",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "generate:routes": "vite-node --config vite.routes.config.ts --mode production scripts/generate-routes.ts",
    "generate:bundle-map": "GENERATE_BUNDLE_MAP=true yarn build-only",
    "generate:dependency-graph": "depcruise $0 --focus $0 --focus-depth 0 --reaches $1 --config .dependency-graph.cjs --output-type dot | dot -T svg | depcruise-wrap-stream-in-html > artifacts/dependency-graph.html",
    "storybook:dev": "yarn generate:certificates && storybook dev --port 6006 --no-open --https --ssl-cert ./.certificates/public.pem --ssl-key ./.certificates/private.pem",
    "storybook:build": "storybook build",
    "storybook:compress": "vite-node --mode production scripts/compress-storybook.ts --",
    "postinstall": "husky"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@builder.io/sdk-vue": "^0.3.1",
    "@headlessui/vue": "^1.7.17",
    "@hotjar/browser": "^1.0.9",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/container-queries": "^0.1.1",
    "@unhead/vue": "^1.8.10",
    "@vee-validate/rules": "~4.12.6",
    "@vee-validate/yup": "~4.12.6",
    "@vue/apollo-composable": "^4.0.1",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "@vueuse/integrations": "^10.7.2",
    "axios": "^1.6.7",
    "dompurify": "^3.0.8",
    "flag-icons": "^7.1.0",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^11.2.0",
    "maska": "^2.1.11",
    "skyflow-js": "^1.34.2",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "11.0.5",
    "tw-elements": "^1.1.0",
    "utility-types": "^3.11.0",
    "uuid": "^9.0.1",
    "vee-validate": "~4.12.6",
    "vue": "~3.4.15",
    "vue-html-secure": "^1.0.10",
    "vue-i18n": "^9.9.1",
    "vue-router": "~4.2.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@graphql-codegen/add": "5.0.2",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/introspection": "4.0.2",
    "@graphql-codegen/named-operations-object": "3.0.0",
    "@graphql-codegen/typed-document-node": "5.0.4",
    "@graphql-codegen/typescript": "4.0.4",
    "@graphql-codegen/typescript-operations": "4.1.2",
    "@rollup/plugin-graphql": "^2.0.4",
    "@rushstack/eslint-patch": "^1.3.3",
    "@storybook/addon-a11y": "^8.0.8",
    "@storybook/addon-actions": "^8.0.8",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-viewport": "^8.0.8",
    "@storybook/cli": "^8.0.8",
    "@storybook/preview-api": "^8.0.8",
    "@storybook/test": "8.0.8",
    "@storybook/vue3": "^8.0.8",
    "@storybook/vue3-vite": "^8.0.8",
    "@tsconfig/node20": "^20.1.2",
    "@types/adm-zip": "^0.5.5",
    "@types/dompurify": "^3.0.5",
    "@types/gtag.js": "^0.0.18",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "~20.11.15",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "~5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "~2.4.4",
    "@vue/tsconfig": "~0.5.1",
    "adm-zip": "^0.5.10",
    "autoprefixer": "^10.4.17",
    "chalk": "^5.3.0",
    "dependency-cruiser": "^16.1.0",
    "eslint": "^8.49.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-vue": "^9.17.0",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "husky": "^9.0.7",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "storybook": "^8.0.8",
    "storybook-vue3-router": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "vite": "~5.0.11",
    "vite-node": "~1.2.2",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-favicon2": "^1.1.5",
    "vite-plugin-mkcert": "^1.17.3",
    "vitest": "~1.2.2",
    "vue-component-type-helpers": "^2.0.6",
    "vue-tsc": "~1.8.27"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.11.0 <21.0.0"
  },
  "packageManager": "yarn@4.1.0"
}
