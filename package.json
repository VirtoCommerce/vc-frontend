{
  "name": "client-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint-staged": "lint-staged",
    "init-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint --fix packages/**/*.{js,ts,vue}",
    "compress": "vue-tsc --noEmit && vite build && gulp compress",
    "generate:graphql": "graphql-codegen -c graphql.codegen.yml && eolConverter crlf \"client-app/core/api/graphql/*\"",
    "i18n:report": "vue-cli-service i18n:report --src './client-app/**/*.?(js|vue)' --locales './client-app/locales/**/*.json'",
    "nswag": "nswag run"
  },
  "dependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/roboto": "^4.5.1",
    "@rollup/plugin-graphql": "^1.1.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.6.5",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.6",
    "vue": "^3.2.16",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^2.4.2",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "eslint": "^7.31.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.14.0",
    "fse": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-exclude-gitignore": "^1.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.0.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "merge2": "^1.4.1",
    "nswag": "^13.6.2",
    "postcss": "^8.4.4",
    "prettier": "^2.3.2",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-simple-gql": "^0.5.0",
    "vue-tsc": "^0.3.0",
    "yarn": "^1.22.11"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  }
}
