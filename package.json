{
  "name": "vc-theme-b2b-vue",
  "version": "1.22.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "yarn type-check && vite build",
    "build:dev": "vite build --mode=development",
    "build:watch": "vite build --mode=development --watch",
    "compress": "yarn build && gulp compress",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint-staged": "lint-staged",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "generate:graphql:schema": "graphql-codegen -c \"codegen.schema.ts\" -r dotenv/config",
    "generate:graphql:types": "graphql-codegen -c \"codegen.types.ts\" -r dotenv/config",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "storybook:dev": "start-storybook -p 6006 --no-open --disable-telemetry",
    "storybook:build": "build-storybook",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@headlessui/vue": "^1.7.10",
    "@popperjs/core": "^2.11.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vee-validate/rules": "^4.7.4",
    "@vueuse/components": "^9.13.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^0.9.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.28.0",
    "dompurify": "^3.0.0",
    "flag-icons": "^6.6.6",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^4.2.12",
    "maska": "^1.5.2",
    "moment": "^2.29.4",
    "swiper": "^8.4.7",
    "vee-validate": "~4.5.11",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@graphql-codegen/add": "^4.0.0",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/introspection": "^3.0.0",
    "@graphql-codegen/typescript": "^3.0.0",
    "@graphql-codegen/typescript-operations": "^3.0.0",
    "@rollup/plugin-graphql": "^2.0.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/cli": "^6.5.16",
    "@storybook/react-docgen-typescript-plugin": "^1.0.1",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.16",
    "@types/dompurify": "^2.4.0",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash": "^4.14.191",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.34.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tailwindcss": "^3.9.0",
    "eslint-plugin-vue": "^9.9.0",
    "eslint-plugin-vuejs-accessibility": "~2.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "optionator": "^0.9.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.8.4",
    "vite": "^4.1.3",
    "vite-plugin-checker": "0.5.4",
    "vite-plugin-mkcert": "^1.13.2",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "1.0.24",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "@types/react": "file:stub/@types/react"
  }
}
