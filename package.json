{
  "name": "vc-theme-b2b-vue",
  "version": "1.21.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "yarn type-check && vite build",
    "build:dev": "vite build --mode=development",
    "build:watch": "vite build --mode=development --watch",
    "compress": "yarn build && gulp compress",
    "type-check": "vue-tsc --noEmit --extendedDiagnostics -p tsconfig.json --composite false",
    "lint-staged": "lint-staged",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "generate:graphql:schema": "graphql-codegen -c \"codegen.schema.ts\" -r dotenv/config && eolConverter crlf \"client-app/xapi/schema.json\"",
    "generate:graphql:types": "graphql-codegen -c \"codegen.types.ts\" -r dotenv/config && eolConverter crlf \"client-app/xapi/types.ts\"",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@headlessui/vue": "^1.7.4",
    "@popperjs/core": "^2.11.6",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vee-validate/rules": "^4.7.3",
    "@vueuse/components": "^9.5.0",
    "@vueuse/core": "^9.5.0",
    "@vueuse/head": "^0.9.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.26.1",
    "dompurify": "^2.4.1",
    "flag-icons": "^6.6.6",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^4.2.2",
    "maska": "^1.5.0",
    "moment": "^2.29.4",
    "swiper": "^8.4.4",
    "vee-validate": "~4.5.11",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.11",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.1",
    "@graphql-codegen/typescript-operations": "^2.5.6",
    "@rollup/plugin-graphql": "^2.0.2",
    "@types/dompurify": "^2.3.4",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash": "^4.14.188",
    "@types/marked": "^4.0.7",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "eol-converter-cli": "^1.0.8",
    "eslint": "^8.27.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.5",
    "vite-plugin-checker": "0.5.4",
    "vite-plugin-mkcert": "^1.11.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  }
}