    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-html-secure": "^1.0.10",
    "vue-i18n": "^11.1.8",
    "vue-router": "^4.5.1",
    "vue3-application-insights": "^1.1.0",
    "yup": "^1.6.1"
