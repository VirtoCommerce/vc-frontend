{
  "name": "vc-theme-b2b-vue",
  "version": "1.40.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "yarn type-check && vite build",
    "build:dev": "vite build --mode=development",
    "build:watch": "vite build --mode=development --watch",
    "compress": "yarn build && gulp compress",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json",
    "lint-staged": "lint-staged",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "generate:graphql:schema": "graphql-codegen --config graphql-codegen.schema.config.ts --require ./scripts/graphql-codegen.js",
    "generate:graphql:types": "graphql-codegen --config graphql-codegen.types.config.ts --require ./scripts/graphql-codegen.js",
    "generate:graphql": "yarn generate:graphql:schema && yarn generate:graphql:types",
    "generate:routes": "vite-node scripts/generate-routes.ts --mode production",
    "storybook:dev": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --docs",
    "postinstall": "husky install",
    "postbuild": "yarn generate:routes"
  },
  "dependencies": {
    "@builder.io/sdk-vue": "^0.3.1",
    "@headlessui/vue": "^1.7.13",
    "@popperjs/core": "^2.11.8",
    "@vee-validate/rules": "^4.11.0",
    "@vee-validate/yup": "^4.11.0",
    "@vueuse/components": "^10.1.2",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "~0.9.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.30.2",
    "dompurify": "^3.0.3",
    "eslint-plugin-sort-export-all": "^1.4.1",
    "flag-icons": "^6.7.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "~4.3.0",
    "maska": "~1.5.2",
    "moment": "^2.29.4",
    "swiper": "^8.4.7",
    "tw-elements": "^1.0.0-rc.1",
    "vee-validate": "^4.11.0",
    "vue": "^3.3.4",
    "vue-html-secure": "^1.0.10",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@rollup/plugin-graphql": "^2.0.3",
    "@rushstack/eslint-patch": "^1.3.2",
    "@storybook/addon-a11y": "^7.2.0",
    "@storybook/addon-actions": "^7.2.0",
    "@storybook/addon-essentials": "^7.2.0",
    "@storybook/addon-interactions": "^7.2.0",
    "@storybook/addon-links": "^7.2.0",
    "@storybook/cli": "^7.2.0",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.2.0",
    "@storybook/vue3-vite": "7.2.0",
    "@tsconfig/node18": "^18.2.0",
    "@types/dompurify": "^3.0.2",
    "@types/gtag.js": "^0.0.12",
    "@types/lodash": "^4.14.195",
    "@types/marked": "^4.3.1",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.15.1",
    "eslint-plugin-vuejs-accessibility": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.64.2",
    "storybook": "7.2.0",
    "storybook-vue3-router": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vite-node": "^0.33.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-mkcert": "^1.16.0",
    "vue-tsc": "^1.8.6",
    "yarn": "1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "resolutions": {
    "@types/react": "file:stub/@types/react"
  },
  "packageManager": "yarn@3.6.1"
}