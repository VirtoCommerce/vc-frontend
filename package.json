{
  "name": "vc-theme-b2b-vue",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "dev": "vite",
    "dev-expose": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "builder": "vue-tsc --noEmit && vite build --mode=development",
    "serve": "vite preview",
    "lint-staged": "lint-staged",
    "init-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint": "eslint \"client-app/**/*.{js,ts,vue}\"",
    "lint:fix": "eslint --fix \"client-app/**/*.{js,ts,vue}\"",
    "prettier": "prettier --check \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "prettier:fix": "prettier --write \"client-app/**/*.{js,ts,vue,graphql,json}\"",
    "compress": "vue-tsc --noEmit && vite build && gulp compress",
    "generate:graphql": "graphql-codegen -c graphql.codegen.yml -r dotenv/config && eolConverter crlf client-app/xapi/*",
    "i18n:report": "vue-cli-service i18n:report --src './client-app/**/*.?(js|vue)' --locales './client-app/locales/**/*.json'",
    "nswag": "nswag run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@headlessui/vue": "^1.6.4",
    "@popperjs/core": "^2.11.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@vueuse/core": "^8.6.0",
    "@vueuse/head": "^0.7.6",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "core-js": "^3.22.8",
    "dompurify": "^2.3.8",
    "flag-icons": "^6.3.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "marked": "^4.0.16",
    "maska": "^1.5.0",
    "moment": "^2.29.1",
    "swiper": "^8.2.2",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.5.0",
    "@graphql-codegen/typescript-operations": "^2.4.1",
    "@rollup/plugin-graphql": "^1.1.0",
    "@types/dompurify": "^2.3.3",
    "@types/lodash": "^4.14.182",
    "@types/marked": "^4.0.3",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "chalk": "^5.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eol-converter-cli": "^1.0.8",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "fse": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-zip": "^5.0.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "nswag": "^13.16.1",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "sass": "^1.52.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.7.3",
    "vite": "^2.9.10",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.37.3",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{graphql,json}": [
      "prettier --write"
    ]
  }
}
